      sizes="512x512"
      href="img/favicon/android-chrome-512x512"
    />
    <link
      rel="icon"
      type="image/png"
      sizes="16x16"
      href="img/favicon/favicon-16x16.png"
    />
    <link rel="manifest" href="/favicon/site.webmanifest" />

    <!-- Мета-данные сайта (для поисковиков) -->
    <title><%=data4index.siteTitle%></title>
    <meta name="description" content="<%=data4index.metaDescr%>" />
    <meta name="keywords" content="<%=data4index.metaKeyWords%>" />

    <!-- Необязательные -->
    <meta name="rating" content="general" />
    <meta name="copyright" content="Copyright 2023" />

    <!-- Для мобильных устройств -->
    <meta name="format-detection" content="telephone=no" />
    <meta name="HandheldFriendly" content="true" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <!-- http-equiv -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />

    <!-- Подключаем CSS, JQuery -->
    <link href="css, scss/Styles.css" rel="stylesheet" type="text/css" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.1/jquery.min.js"></script>

    <!-- Подключаем доп. файл с функциями -->
